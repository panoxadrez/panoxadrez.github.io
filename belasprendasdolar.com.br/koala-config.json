// Sass project settings, you can edit it and set custom settings.
{
	// The mappings of source directory and output directory
	"mappings": [
		{
			"src": "/scss/",
			"dest": "/css/"
		}
	],
	"ignores": [
		"*.css",
		"bundle",
		"lib",
		"html",
		"ly-0001",
		"old"
	],
	"options": {
		"sourceMap": false,
		"autoprefix": false,
		"autoprefixConfig": "> 1%, last 2 versions, Firefox ESR, Opera 12.1"
	}
}