{
	"mappings": [
		{
			"src": "/scss/",
			"dest": "/css/"
		}
	],
	"ignores": [
		"css/",
		"_test/",
		"*.css",
		"bundle",
		"lib",
		"html",
		"ly-0001",
		"old",
		"campanhas/000_default",
		"campanhas/000001",
		"campanhas/000002"
	],
	"options": {
		"sourceMap": false,
		"autoprefix": false,
		"autoprefixConfig": "> 1%, last 2 versions, Firefox ESR, Opera 12.1"
	}
}